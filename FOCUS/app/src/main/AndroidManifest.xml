<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="abdelsattar.com.focusII">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="abdelsattar.com.focusII.Activties.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="abdelsattar.com.focusII.Activties.ToDoList"
            android:label="@string/title_activity_to_do_list"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="abdelsattar.com.focusII.Activties.subtasksDetails"
            android:label="Sub-Tasks"
            />
        <activity android:name="abdelsattar.com.focusII.Activties.TopTasks"
            android:label="Top 3 Tasks"
            />
        <activity android:name="abdelsattar.com.focusII.Activties.Contact"
            android:label="Contact"
            />
        <activity android:name="abdelsattar.com.focusII.Activties.Thankful"
            android:label="Thankful for"
            />
        <activity android:name="abdelsattar.com.focusII.Activties.About"/>
    </application>

</manifest>